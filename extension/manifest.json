{
  "name": "Pubs importer",
  "description": "Imports the contents of current tab into pubs",
  "version": "0.2",
  "permissions": [
    "activeTab", "nativeMessaging","contextMenus", "declarativeContent"
  ],
  "background": {
    "scripts": ["connector.js", "background.js"]
  },
  "key": "",
  "page_action": {
      "default_title": "Import to pubs",
      "default_icon": "print_16x16.png"  },
  "manifest_version": 2
}
