{
  "name": "Pubs importer",
  "description": "Imports the contents of current tab into pubs",
  "version": "1.2",
  "permissions": [
    "activeTab", "nativeMessaging"
  ],
  "browser_action": {
      "default_title": "Import to pubs",
      "default_icon": "print_16x16.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
